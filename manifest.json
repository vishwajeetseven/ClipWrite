{
  "manifest_version": 3,
  "name": "ClipWrite",
  "version": "1.0",
  "description": "Simulate.",
  "permissions": [
    "contextMenus",
    "scripting",
    "clipboardRead",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "commands": {
    "activate-writer": {
      "suggested_key": {
        "default": "Ctrl+Shift+5"
      },
      "description": "Activate ClipWrite"
    }
  }
}
